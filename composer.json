{
  "name": "loay-ghanem/laravel-pkg",
  "description": "A Laravel OTP service for phone/email verification",
  "type": "library",
  "license": "Apache-2.0",
  "autoload": {
      "psr-4": {
          "LoayGhanem\\OtpService\\": "src/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "LoayGhanem\\OtpService\\OtpServiceProvider"
          ],
          "aliases": {
              "Otp": "LoayGhanem\\OtpService\\Facades\\Otp"
          }
      }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "*"
  }
}
