{
  "name": "loay-ghanem/laravel-pkg",
  "description": "A Laravel OTP service for phone/email verification",
  "type": "library",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "YourTeam\\OtpService\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourTeam\\OtpService\\OtpServiceProvider"
      ],
      "aliases": {
        "Otp": "YourTeam\\OtpService\\Facades\\Otp"
      }
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
  }
}
